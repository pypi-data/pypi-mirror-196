#!/usr/bin/env python

import argh
import numpy as np

from serpent.cli import commands

parser = argh.ArghParser()
parser.add_commands(commands.__all__)


def main():
	np.set_printoptions(
		precision=8,
		suppress=True,
		sign=' ',
		edgeitems=128,
		linewidth=320,
	)
	parser.dispatch()


if __name__ == '__main__':
	main()
